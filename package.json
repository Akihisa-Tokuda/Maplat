{
  "name": "@maplat/ui",
  "version": "0.6.13",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/",
    "dist/",
    "legacy/",
    "less/",
    "parts/",
    "fonts/",
    "locales/"
  ],
  "scripts": {
    "server": "gulp server",
    "stopserver": "gulp stopserver",
    "less_build": "lessc --clean-css less/ui.less dist/maplat.css",
    "less_build_packed": "lessc --clean-css less/ui_packed.less dist_packed/maplat.css",
    "www_build": "webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "www_build_packed": "webpack --config ./webpack_config/webpack.config.packed.js",
    "zip_build": "gulp zip",
    "sw_build": "workbox injectManifest",
    "build": "npm run freeze && npm run less_build && npm run www_build && npm run sw_build && npm run zip_build",
    "build_packed": "npm run freeze && npm run less_build_packed && npm run www_build_packed",
    "lint": "eslint src/",
    "preversion": "npm run lint",
    "version": "npm run build && npm run build_packed && git add -A",
    "freeze": "node node_modules/@maplat/core/bin/freezer.js -s parts -f src/freeze_images.js -u src/pointer_images.js && node node_modules/@maplat/core/bin/freezer.js -s locales -f src/freeze_locales.js -i",
    "preinstall": "npx npm-force-resolutions || echo 'ignore'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Maplat.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/code4history/Maplat/issues"
  },
  "homepage": "https://github.com/code4history/Maplat/wiki",
  "dependencies": {
    "@maplat/core": "github:code4history/MaplatCore#weiwudi",
    "@maplat/tin": "^0.6.7",
    "@turf/turf": "^5.1.6",
    "caniuse-lite": "^1.0.30001120",
    "lodash.template": "^4.5.0",
    "ol": "^6.1.1",
    "ol5": "npm:ol@^5.3.3",
    "swiper": "^5.3.1",
    "weiwudi": "0.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-loader": "^8.2.2",
    "clean-css": "^4.2.3",
    "core-js": "^3.8.0",
    "eslint": "^6.8.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-replace": "^1.0.0",
    "gulp-zip": "^4.2.0",
    "jasmine": "^3.5.0",
    "less": "^3.12.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "psaux": "^0.4.0",
    "sha1": "^1.1.1",
    "terminate": "^2.1.2",
    "utils-extend": "npm:utils-extend-patched@^1.0.9",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.4.0",
    "workbox-build": "^5.1.4",
    "workbox-cli": "^5.1.4",
    "yargs-parser": "^18.1.3"
  },
  "resolutions": {
    "clean-css": "^4.2.3",
    "minimist": "^1.2.5",
    "yargs-parser": "^18.1.3",
    "utils-extend": "npm:utils-extend-patched@^1.0.9"
  }
}
