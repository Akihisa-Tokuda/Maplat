{
  "name": "@maplat/ui",
  "version": "0.7.4",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/",
    "dist/",
    "legacy/",
    "less/",
    "parts/",
    "fonts/",
    "locales/"
  ],
  "scripts": {
    "server": "webpack-dev-server --config ./webpack_config/webpack.config.standard.dev.js",
    "server:packed": "webpack-dev-server --config ./webpack_config/webpack.config.packed.dev.js",
    "www_build": "webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "www_build:packed": "webpack --config ./webpack_config/webpack.config.packed.js",
    "zip_build": "gulp zip",
    "sw_build": "workbox injectManifest",
    "build": "npm run www_build && npm run sw_build && npm run zip_build",
    "build:packed": "npm run www_build:packed",
    "test": "jest",
    "preversion": "npm run lint",
    "version": "npm run build && npm run build_packed && git add -A",
    "freeze": "node node_modules/@maplat/core/bin/freezer.js -s locales -f src/freeze_locales.js -i",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.js\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.js\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Maplat.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/code4history/Maplat/issues"
  },
  "homepage": "https://github.com/code4history/Maplat/wiki",
  "dependencies": {
    "@maplat/core": "^0.8.3",
    "@maplat/tin": "^0.7.1",
    "@turf/turf": "^5.1.6",
    "bootstrap.native": "^3.0.14",
    "caniuse-lite": "^1.0.30001120",
    "lodash.template": "^4.5.0",
    "ol": "^6.1.1",
    "ol5": "npm:ol@^5.3.3",
    "swiper": "^5.3.1",
    "weiwudi": "^0.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "file-loader": "^6.2.0",
    "font-loader": "^0.1.2",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-zip": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "less": "^3.13.1",
    "less-loader": "^7.1.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "workbox-build": "^6.0.2",
    "workbox-cli": "^5.1.4"
  }
}
